{
  "name": "@vswift/components",
  "version": "0.1.3-3",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "component",
    "vue",
    "element-plus",
    "radash"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s clean type-emit \"build-only {@}\" --",
    "release": "npm publish",
    "build-only": "vite build",
    "type-emit": "vue-tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.7.0",
    "radash": "^12.1.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.5.1",
    "glob": "^10.3.12",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "vite-plugin-inspect": "^0.8.4",
    "vue-tsc": "^2.0.11"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./myLabel": {
      "import": "./dist/my-label.js",
      "require": "./dist/my-label.cjs",
      "types": "./dist/types/my-label/index.d.ts"
    },
    "./myDividerTitle": {
      "import": "./dist/my-divider-title.js",
      "require": "./dist/my-divider-title.cjs",
      "types": "./dist/types/my-divider-title/index.d.ts"
    },
    "./myColorPicker": {
      "import": "./dist/my-color-picker.js",
      "require": "./dist/my-color-picker.cjs",
      "types": "./dist/types/my-color-picker/index.d.ts"
    }
  }
}