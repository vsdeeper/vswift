  "devDependencies": {
    "@vswift/cli": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "release-it": "^15.5.1",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.3",
