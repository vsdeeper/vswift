    "execa": "^6.1.0",
    "nanospinner": "^1.1.0",
    "chalk": "^5.2.0",
    "consola": "^2.15.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.14",
    "@types/shelljs": "^0.8.11",
    "typescript": "^4.6.4",
    "release-it": "^15.5.1"
  }
}