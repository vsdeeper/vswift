{
  "name": "@vswift/cli",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist",
    "bin.js"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vswift-cli": "./bin.js"
  },
  "publishConfig": {
    "access": "public",
