{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "examples/src/**/*", "examples/src/**/*.vue", "auto-imports.d.ts"],
  "exclude": ["examples/src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.examples.tsbuildinfo",
